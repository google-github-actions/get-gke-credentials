{
  "name": "get-gke-credentials",
  "version": "1.0.0",
  "description": "Get short lived credentials to a GKE cluster",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build -m src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "fixlint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 600s 'tests/*.test.ts'",
    "e2e-tests": "mocha -r ts-node/register -t 120s 'tests/e2e/e2e.test.ts'",
    "cleanup": "mocha -r ts-node/register -t 240s 'tests/cleaner/cleanTestFunctions.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/get-gke-credentials"
  },
  "keywords": [
    "gke",
    "kubernetes",
    "GCP",
    "google cloud",
    "Google Kubernetes Engine"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@google-github-actions/actions-utils": "^0.4.3",
    "google-auth-library": "^8.5.2",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.17.1",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.5",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vercel/ncc": "^0.34.0",
    "chai": "^4.3.6",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "sinon": "^15.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
