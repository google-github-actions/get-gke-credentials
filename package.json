{
  "name": "get-gke-credentials",
  "version": "0.3.0",
  "description": "Get short lived credentials to a GKE cluster",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "fixlint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 600s 'tests/*.test.ts'",
    "e2e-tests": "mocha -r ts-node/register -t 120s 'tests/e2e/e2e.test.ts'",
    "cleanup": "mocha -r ts-node/register -t 240s 'tests/cleaner/cleanTestFunctions.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/get-gke-credentials"
  },
  "keywords": [
    "gke",
    "kubernetes",
    "GCP",
    "google cloud",
    "Google Kubernetes Engine"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "fs": "0.0.1-security",
    "google-auth-library": "^7.9.2",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.15.1",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/lodash": "^4.14.173",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/sinon": "^10.0.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.1",
    "@zeit/ncc": "^0.22.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.1",
    "prettier": "^2.4.0",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "yaml": "^1.10.2"
  }
}
