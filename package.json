{
  "name": "get-gke-credentials",
  "version": "0.7.0",
  "description": "Get short lived credentials to a GKE cluster",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build -m src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "fixlint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 600s 'tests/*.test.ts'",
    "e2e-tests": "mocha -r ts-node/register -t 120s 'tests/e2e/e2e.test.ts'",
    "cleanup": "mocha -r ts-node/register -t 240s 'tests/cleaner/cleanTestFunctions.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google-github-actions/get-gke-credentials"
  },
  "keywords": [
    "gke",
    "kubernetes",
    "GCP",
    "google cloud",
    "Google Kubernetes Engine"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@google-github-actions/actions-utils": "^0.1.6",
    "google-auth-library": "^7.14.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vercel/ncc": "^0.33.3",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
